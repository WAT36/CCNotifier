generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model balance {
  symbol         String   @unique
  amount         Float
  conversionRate Float
  updated_at     DateTime @default(now()) @db.Timestamp(6)
}
