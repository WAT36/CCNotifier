generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["views"]
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model transaction {
  id               String   @id
  brand            String
  buysell          String
  amount           Float
  rate             Float
  payment          Float
  transaction_date DateTime @db.Timestamp(6)
}

/// The underlying view does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
view brand {
  name String @unique
}

/// The underlying view does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
view latest_transaction {
  id               String   @id
  brand            String   @unique
  buysell          String
  amount           Float
  rate             Float
  payment          Float
  transaction_date DateTime @db.Timestamp(6)
}

/// The underlying view does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
view latest_sell_transaction {
  id               String   @id
  brand            String   @unique
  buysell          String
  amount           Float
  rate             Float
  payment          Float
  transaction_date DateTime @db.Timestamp(6)
}

/// The underlying view does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
view now_bet {
  brand String @unique
  bet   Float
}
