generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model transaction {
  id               String   @id
  brand            String
  buysell          String
  amount           Float
  rate             Float
  payment          Float
  transaction_date DateTime @db.Timestamp(6)
}
