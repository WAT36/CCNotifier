generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["views"]
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model tradeHistory {
  trade_date                  DateTime @db.Timestamp(6)
  settlement_category         String
  yen_payment                 Float
  order_id                    String
  contract_id                 String
  position_id                 String
  brand                       String
  order_type                  String
  trade_category              String
  buysell_category            String
  ioc_category                String
  contract_amount             Float
  contract_rate               Float
  contract_payment            Float
  order_commission            Float
  leverage_commission         Float
  deposit_withdrawal_category String
  deposit_withdrawal_amount   Float
  givetake_category           String
  givetake_amount             Float
  sending_commission          Float
  sender                      String
  transaction_id              String
  id                          Int      @id @default(autoincrement())
}

model brandBidAsk {
  brand            String    @unique @db.VarChar(3)
  bid_price        Float?
  ask_price        Float?
  bid_updated_time DateTime? @db.Timestamp(6)
  ask_updated_time DateTime? @db.Timestamp(6)
}

/// The underlying view does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
view brand {
  name String @unique
}

/// The underlying view does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
view latest_shop_trade {
  id                  Int      @unique
  brand               String
  settlement_category String
  buysell_category    String
  yen_payment         Float
  contract_amount     Float
  contract_rate       Float
  contract_payment    Float
  trade_date          DateTime @db.Timestamp(6)
}

/// The underlying view does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
view latest_shop_sell_trade {
  id                  Int      @unique
  brand               String
  settlement_category String
  buysell_category    String
  yen_payment         Float
  contract_amount     Float
  contract_rate       Float
  contract_payment    Float
  trade_date          DateTime @db.Timestamp(6)
}

/// The underlying view does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
view now_bet {
  brand           String @unique
  contract_amount Float
  givetake_amount Float
  now_amount      Float
  yen_bet         Float
}
