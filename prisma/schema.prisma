generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["views"]
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model tradeHistory {
  trade_date                  DateTime @db.Timestamp(6)
  settlement_category         String
  yen_payment                 Decimal
  order_id                    String
  contract_id                 String
  position_id                 String
  brand                       String
  order_type                  String
  trade_category              String
  buysell_category            String
  ioc_category                String
  contract_amount             Decimal
  contract_rate               Decimal
  contract_payment            Decimal
  order_commission            Decimal
  leverage_commission         Decimal
  deposit_withdrawal_category String
  deposit_withdrawal_amount   Decimal
  givetake_category           String
  givetake_amount             Decimal
  sending_commission          Decimal
  sender                      String
  transaction_id              String
  id                          Int      @id @default(autoincrement())
}

model brandBidAsk {
  brand            String    @unique @db.VarChar(3)
  bid_price        Decimal?
  ask_price        Decimal?
  bid_updated_time DateTime? @db.Timestamp(6)
  ask_updated_time DateTime? @db.Timestamp(6)
}

/// The underlying view does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
view brand {
  name String @unique
}

/// The underlying view does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
view latest_shop_trade {
  id                  Int      @unique
  brand               String
  settlement_category String
  buysell_category    String
  yen_payment         Decimal
  contract_amount     Decimal
  contract_rate       Decimal
  contract_payment    Decimal
  trade_date          DateTime @db.Timestamp(6)
}

/// The underlying view does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
view latest_shop_sell_trade {
  id                  Int      @unique
  brand               String
  settlement_category String
  buysell_category    String
  yen_payment         Decimal
  contract_amount     Decimal
  contract_rate       Decimal
  contract_payment    Decimal
  trade_date          DateTime @db.Timestamp(6)
}

/// The underlying view does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
view now_bet {
  brand           String  @unique
  contract_amount Decimal
  givetake_amount Decimal
  now_amount      Decimal
  yen_bet         Decimal
}
